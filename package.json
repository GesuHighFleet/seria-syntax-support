{
  "name": "seria-syntax-support",
  "displayName": "SERIA Syntax Support",
  "description": "An extension to support syntax highlighting of .seria files of Haaf's Game Engine, most famously used by the game HighFleet",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "seria",
      "aliases": [".seria", "seria"],
      "extensions": [".seria"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "seria",
      "scopeName": "source.seria",
      "path": "./syntaxes/seria.tmLanguage.json"
    }]
  }
}
